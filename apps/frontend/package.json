{
  "name": "frontend",
  "version": "0.1.0",
  "packageManager": "pnpm@9.6.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"next dev -p 4000\" \"pnpm generate:watch\"",
    "build": "NEXT_DISABLE_ESLINT=true NEXT_DISABLE_TYPECHECK=true next build",
    "build:backend": "turbo run build ---filter=backend",
    "start": "next start -p 4000",
    "lint": "next lint",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "generate": "graphql-codegen --config codegen.ts",
    "generate:watch": "graphql-codegen --config codegen.ts --watch"
  },
  "dependencies": {
    "@apollo/client": "3.10.0-rc.1",
    "@apollo/experimental-nextjs-app-support": "^0.10.0",
    "@auth/prisma-adapter": "^2.8.0",
    "@auth/supabase-adapter": "^0.7.0",
    "@bazika/db": "workspace:*",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@mynaui/icons-react": "^0.2.1",
    "@prisma/client": "5.17.0",
    "@prisma/extension-accelerate": "^1.2.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tanstack/react-query": "^5.80.5",
    "@tanstack/react-query-devtools": "^5.80.5",
    "@tanstack/react-table": "^8.21.3",
    "@types/google-one-tap": "^1.2.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vidstack/react": "^1.12.13",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "embla-carousel-autoplay": "^8.1.8",
    "embla-carousel-react": "^8.0.2",
    "framer-motion": "^11.3.22",
    "geist": "^1.3.0",
    "google-auth-library": "^9.8.0",
    "graphql": "^16.8.1",
    "hls.js": "^1.5.20",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.371.0",
    "next": "14.2.1",
    "next-auth": "^4.24.7",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.2.3",
    "react-icons": "^5.1.0",
    "react-player": "^2.16.0",
    "react-resizable-panels": "^2.1.2",
    "recharts": "^2.15.3",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/client-preset": "^4.0.1",
